{{$meta := .Meta}}
{{$values := (raw_value_of .ResourceValue .Meta)}}
{{range $index, $value := $values}}
  {{$url := url_for $value}}
  {{if (and $url (defined_resource_show_page $meta.Resource))}}
    <a data-url="{{$url}}" href="{{$url}}">{{stringify $value}}</a>
  {{else}}
    {{stringify $value}}
  {{end}}
{{end}}
