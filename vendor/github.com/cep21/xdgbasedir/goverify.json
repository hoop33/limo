{
  "ignoreDir": ["Godeps"],
  "checks": [
    {
      "macro": "goimport"
    }, {
      "macro": "gofmt"
    }, {
      "macro": "vet"
    }, {
      "macro": "golint"
    }, {
      "macro": "gocyclo"
    }, {
      "macro": "go-cover",
      "validate": {
        "type": "cover",
        "coverage": 50
      }
    }
  ]
}
